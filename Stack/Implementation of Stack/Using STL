#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main()
{
    stack<int>st;  //declared stack
    
    //push elements in stack
    st.push(1);
    st.push(2);
    st.push(3);
    st.push(4);
    st.push(5);
    
    //pop from Stack
    st.pop();
    
    //top element of Stack
    cout<<"Top Element: "<<st.top()<<endl<<endl;
    
    //stack is empty or not
    cout<<"Is Stack Empty: "<<st.empty()<<endl<<endl;
    
    //size of stack
    cout<<"Stack Size: "<<st.size()<<endl<<endl;
    
    //Printning Stack Elements
    cout<<"Stack Elements are: ";
    
    while(!st.empty()){
        cout<<st.top()<<" ";
        st.pop();
    }

    return 0;
}
